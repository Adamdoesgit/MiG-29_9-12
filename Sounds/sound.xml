<!-- СПО-15ЛМ  -->
  <SPO15>
   <name>SPO15_lockon</name>
   <type>avionics</type>
   <path>Aircraft/MiG-29_9-12/Sounds/avionics/SPO-15_lockon.wav</path>
   <mode>looped</mode>
   <condition>
    <property>/mig29/instrumentation/SPO-15LM/lockon</property>
    <value>1</value>
   </condition>
  </SPO15>
  <SPO15>
   <name>SPO15_fire</name>
   <type>avionics</type>
   <path>Aircraft/MiG-29_9-12/Sounds/avionics/SPO-15_fire.wav</path>
   <mode>looped</mode>
   <condition>
    <property>/mig29/instrumentation/SPO-15LM/fire</property>
    <value>1</value>
   </condition>
  </SPO15>

