<?xml version="1.0"?>

<PropertyList>

 <fx>

  <!-- Двигатели -->
  <engine>
   <name>Engine1</name>
   <path>Aircraft/MiG-29_9-12/Sounds/engines/RD-33.wav</path>
   <mode>looped</mode>
   <volume>
    <property>/mig29/systems/engines/Sn2_Left</property>
    <factor>0.002</factor>
   </volume>
   <pitch>
    <property>/engines/engine[0]/n2</property>
    <factor>0.003</factor>
    <offset>0.15</offset>
   </pitch>
  </engine>

  <engine>
   <name>Engine2</name>
   <path>Aircraft/MiG-29_9-12/Sounds/engines/RD-33.wav</path>
   <mode>looped</mode>
   <volume>
    <property>/mig29/systems/engines/Sn2_Right</property>
    <factor>0.002</factor>
   </volume>
   <pitch>
    <property>/engines/engine[1]/n2</property>
    <factor>0.003</factor>
    <offset>0.15</offset>
   </pitch>
  </engine>

  <engine>
   <name>Engine3</name>
   <path>Sounds/jet.wav</path>
   <mode>looped</mode>
   <volume>
    <property>/mig29/systems/engines/Sn2_GTDE</property>
    <factor>0.012</factor>
   </volume>
   <pitch>
    <property>/engines/engine[2]/n2</property>
    <factor>0.03</factor>
   </pitch>
  </engine>

  <engine>
   <name>whine</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <condition>
    <property>/engines/engine[0]/running</property>
   </condition>
   <volume>
    <property>/engines/engine[0]/n2</property>
    <type>log</type>
    <factor>0.1</factor>
    <max>0.75</max>
   </volume>
   <pitch>
    <property>/engines/engine[0]/n2</property>
    <type>lin</type>
    <factor>0.008</factor>
   </pitch>
  </engine>
  <rumble>
   <name>rumble</name>
   <path>Sounds/rumble.wav</path>
   <mode>looped</mode>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <type>lin</type>
    <factor>0.012</factor>
    <max>1.0</max>
   </volume>
  </rumble>

  <!-- Шасси -->
  <gear>
   <name>gear</name>
   <path>Sounds/gear.wav</path>
   <mode>in-transit</mode>
   <property>/gear/gear[0]/position-norm</property>
   <pitch>
    <internal>dt_play</internal>
    <offset>0.1</offset>
    <factor>-0.001</factor>
   </pitch>
  </gear>
  <gear>
   <name>gear-lock</name>
   <path>Sounds/gear-lck.wav</path>
   <condition>
    <equals>
     <property>/gear/gear[0]/position-norm</property>
     <value>0</value>
    </equals>
   </condition>
  </gear>
  <gear>
   <name>gear</name>
   <path>Sounds/gear.wav</path>
   <mode>in-transit</mode>
   <property>/gear/gear[1]/position-norm</property>
   <pitch>
    <internal>dt_play</internal>
    <offset>0.1</offset>
    <factor>-0.001</factor>
   </pitch>
  </gear>
  <gear>
   <name>gear-lock</name>
   <path>Sounds/gear-lck.wav</path>
   <condition>
    <equals>
     <property>/gear/gear[1]/position-norm</property>
     <value>0</value>
    </equals>
   </condition>
  </gear>
  <gear>
   <name>gear</name>
   <path>Sounds/gear.wav</path>
   <mode>in-transit</mode>
   <property>/gear/gear[2]/position-norm</property>
   <pitch>
    <internal>dt_play</internal>
    <offset>0.1</offset>
    <factor>-0.001</factor>
   </pitch>
  </gear>
  <gear>
   <name>gear-lock</name>
   <path>Sounds/gear-lck.wav</path>
   <condition>
    <equals>
     <property>/gear/gear[2]/position-norm</property>
     <value>0</value>
    </equals>
   </condition>
  </gear>

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/speed-down-fps</property>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>

  <!-- Закрылки -->
  <flap>
   <name>flaps</name>
   <path>Sounds/flaps.wav</path>
   <mode>in-transit</mode>
   <property>/surface-positions/flap-pos-norm</property>
   <volume>
    <offset>0.25</offset>
    <max>0.9</max>
   </volume>
  </flap>
  <wind>
   <name>wind</name>
   <path>Sounds/wind.wav</path>
   <mode>looped</mode>
   <property>/velocities/airspeed-kt</property>
   <volume>
    <property>/position/altitude-ft</property>
    <factor>-0.000015</factor>
    <offset>1.0</offset>
    <min>0.1</min>
    <max>1.0</max>
   </volume>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0015</factor>
    <min>0.03</min>
    <max>0.20</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
    <offset>1.25</offset>
   </pitch>
  </wind>

  <cannon>
   <name>cannon</name>
   <path>Sounds/gatling.wav</path>
   <mode>looped</mode>
   <condition>
    <property>controls/armament/trigger</property>
   </condition>
  </cannon>

