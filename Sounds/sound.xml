   <path>Aircraft/MiG-29_9-12/Sounds/Almaz-UP/Proverka.wav</path>
   <mode>looped</mode>
   <condition>
    <property>/mig29/instrumentation/RI/Proverka</property>
    <value>1</value>
   </condition>
  </voice>
