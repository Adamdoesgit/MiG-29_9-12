<?xml version="1.0"?>

<PropertyList>

 <path>Lights.ac</path>

 <nasal>
  <load>
   ##############################################################################
   # The on-load Nasal is not executed when this file is loaded as the user
   # aircraft.
   ##############################################################################
   var rplayer = cmdarg();
   # Set up property aliases for animations.
   rplayer.getNode("mig29/systems/lighting/taxi", 1).
     alias(rplayer.getNode("sim/multiplay/generic/int[7]"));
   rplayer.getNode("mig29/systems/lighting/landing", 1).
     alias(rplayer.getNode("sim/multiplay/generic/int[8]"));
   rplayer.getNode("mig29/systems/lighting/ANO", 1).
     alias(rplayer.getNode("sim/multiplay/generic/int[9]"));
   rplayer.getNode("mig29/systems/lighting/ANObright", 1).
     alias(rplayer.getNode("sim/multiplay/generic/float[10]"));
   ##############################################################################
  </load>
 </nasal>

 <!-- Taxi light cone -->
 <animation>
  <type>select</type>
  <object-name>TTL_Light</object-name>
  <object-name>TTL_halo</object-name>
  <condition>
   <and>
    <equals>
     <property>/sim/rendering/rembrandt/enabled</property>
     <value>1</value>
    </equals>
    <equals>
     <property>mig29/systems/lighting/taxi</property>
     <value>1</value>
    </equals>
   </and>
  </condition>
 </animation>
 <animation>
  <type>light</type>
  <light-type>spot</light-type>
  <name>taxi-light</name>
  <object-name>TTL_Light</object-name>
  <nopreview/>
  <direction>
   <x>-1</x>
   <y> 0</y>
   <z> 0</z>
  </direction>
  <position>
   <x>-5.906</x>
   <y>-0.328</y>
   <z>-1.77717</z>
  </position>
  <ambient>
   <r>0.95</r>
   <g>0.9</g>
   <b>0.9</b>
   <a>1</a>
  </ambient>
  <diffuse>
   <r>0.95</r>
   <g>0.9</g>
   <b>0.9</b>
   <a>1</a>
  </diffuse>
  <specular>
   <r>0.95</r>
   <g>0.9</g>
   <b>0.9</b>
   <a>1</a>
  </specular>
  <attenuation>
   <c>0.5</c>
   <l>0.07</l>
   <q>0</q>
  </attenuation>
  <exponent>30.0</exponent>
  <cutoff>100</cutoff>
  <near-m>0.01</near-m>
  <far-m>100</far-m>
 </animation>
