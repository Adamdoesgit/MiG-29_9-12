<?xml version="1.0"?>

<!-- Tablo 6                                    -->
<!-- Sergey "Mercenary_Mercury" Salov, Aug 2011 -->

<PropertyList>

<path>Tablo-6.ac</path>

<animation>
 <type>noshadow</type>
 <object-name>Tablo-6</object-name>
</animation>

<animation>
 <type>material</type>
 <object-name>Tablo-6</object-name>
 <emission>
  <red>0.8</red>
  <green>0.8</green>
  <blue>0.8</blue>
  <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
 </emission>
</animation>

<animation>
 <type>material</type>
 <object-name>Tablo-6.Navig</object-name>
 <object-name>Tablo-6.Uskor</object-name>
 <object-name>Tablo-6.Transform</object-name>
 <object-name>Tablo-6.CEL</object-name>
 <object-name>Tablo-6.CER</object-name>
 <object-name>Tablo-6.NRCOC</object-name>
 <emission>
  <red>1.0</red>
  <green>1.0</green>
  <blue>1.0</blue>
  <factor-prop>mig29/systems/cockpitlighting/tablo-norm</factor-prop>
 </emission>
</animation>

<animation>
 <type>select</type>
 <object-name>Tablo-6.Navig</object-name>
 <condition>
  <and>
   <greater-than>
    <property>mig29/instrumentation/electrical/v27</property>
    <value>18</value>
   </greater-than>
   <equals>
    <property>mig29/instrumentation/Tablo/NavigReady</property>
    <value>1</value>
   </equals>
  </and>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>Tablo-6.Uskor</object-name>
 <condition>
  <and>
   <greater-than>
    <property>mig29/instrumentation/electrical/v27</property>
    <value>18</value>
   </greater-than>
   <equals>
    <property>mig29/instrumentation/Tablo/AccelReady</property>
    <value>1</value>
   </equals>
  </and>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>Tablo-6.Transform</object-name>
 <condition>
  <and>
   <greater-than>
    <property>mig29/instrumentation/electrical/v27</property>
    <value>18</value>
   </greater-than>
   <equals>
    <property>mig29/instrumentation/Tablo/TransformerFailure</property>
    <value>1</value>
   </equals>
  </and>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>Tablo-6.CEL</object-name>
 <condition>
  <and>
   <greater-than>
    <property>mig29/instrumentation/electrical/v27</property>
    <value>18</value>
   </greater-than>
   <equals>
    <property>mig29/instrumentation/Tablo/ControlIntakeLeft</property>
    <value>1</value>
   </equals>
  </and>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>Tablo-6.CER</object-name>
 <condition>
  <and>
   <greater-than>
    <property>mig29/instrumentation/electrical/v27</property>
    <value>18</value>
   </greater-than>
   <equals>
    <property>mig29/instrumentation/Tablo/ControlIntakeRight</property>
    <value>1</value>
   </equals>
  </and>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>Tablo-6.NRCOC</object-name>
 <condition>
  <and>
   <greater-than>
    <property>mig29/instrumentation/electrical/v27</property>
    <value>18</value>
   </greater-than>
   <equals>
    <property>mig29/instrumentation/Tablo/COCNotReserve</property>
    <value>1</value>
   </equals>
  </and>
 </condition>
</animation>

</PropertyList>