  <object-name>puok.cz1</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>true</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/lighting/tablo-norm</property>
    <step>-0.05</step>
    <min>0</min>
    <max>1</max>
