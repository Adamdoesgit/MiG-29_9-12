   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-R-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>puok.knob1</object-name>
  <property>mig29/controls/lighting/tablo-norm</property>
  <interpolation>
   <entry><ind>0</ind><dep>  0</dep></entry>
   <entry><ind>1</ind><dep>350</dep></entry>
  </interpolation>
  <center>
   <x-m>-0.030</x-m>
   <y-m> 0.000</y-m>
   <z-m> 0.011</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>puok.knob2</object-name>
