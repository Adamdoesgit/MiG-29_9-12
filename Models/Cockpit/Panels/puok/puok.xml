<?xml version="1.0"?>

<PropertyList>

 <path>puok.ac</path>

 <animation>
  <type>material</type>
  <object-name>puok.case</object-name>
  <object-name>puok.knob1</object-name>
  <object-name>puok.knob2</object-name>
  <object-name>puok.knob3</object-name>
  <object-name>puok.knob4</object-name>
  <object-name>puok.pls</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>puok.PTA</object-name>
