  <object-name>PU-189.button5</object-name>
  <object-name>PU-189.button6</object-name>
  <object-name>screws</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>PU-189.Lamp1</object-name>
  <object-name>PU-189.Lamp2</object-name>
  <object-name>PU-189.Lamp3</object-name>
  <object-name>PU-189.Lamp4</object-name>
  <object-name>PU-189.Lamp5</object-name>
  <object-name>PU-189.Lamp6</object-name>
  <emission>
   <red>  1.0</red>
   <green>1.0</green>
   <blue> 1.0</blue>
   <factor-prop>mig29/systems/cockpitlighting/annunciators-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>select</type>
  <object-name>PU-189.Lamp1</object-name>
  <condition>
   <and>
    <greater-than>
     <property>mig29/instrumentation/electrical/v27</property>
     <value>18</value>
    </greater-than>
    <or>
     <equals>
      <property>mig29/controls/lighting/kontr_lamp</property>
      <value>1</value>
     </equals>
     <equals>
      <property>mig29/instrumentation/PU-189/damp</property>
      <value>1</value>
     </equals>
    </or>
   </and>
  </condition>
 </animation>

