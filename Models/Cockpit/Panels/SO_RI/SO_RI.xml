<?xml version="1.0"?>

<PropertyList>

 <path>SO_RI.ac</path>

 <animation>
  <type>material</type>
  <object-name>SO_RI.case</object-name>
  <object-name>SO_RI.basebutton1</object-name>
  <object-name>SO_RI.basebutton2</object-name>
  <object-name>SO_RI.basebutton3</object-name>
  <object-name>RI.button1</object-name>
  <object-name>RI.button2</object-name>
  <object-name>SO-69.button</object-name>
  <object-name>SO_RI.baselever</object-name>
  <object-name>SO_RI.lever</object-name>
  <object-name>SO-69.baselever</object-name>
  <object-name>SO-69.lever</object-name>
  <object-name>SO-69.knob</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>SO_RI.PTA</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-R-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>RI.button1</object-name>
  <property>mig29/controls/RI/repeat</property>
  <factor>0.002</factor>
  <axis>
   <x> 0</x>
   <y> 0</y>
   <z>-1</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>RI.button2</object-name>
  <property>mig29/controls/RI/check</property>
  <factor>0.002</factor>
  <axis>
   <x> 0</x>
   <y> 0</y>
   <z>-1</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>SO-69.button</object-name>
  <property>mig29/controls/SO-69/znak</property>
  <factor>0.002</factor>
  <axis>
   <x> 0</x>
   <y> 0</y>
   <z>-1</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>SO-69.knob</object-name>
  <property>mig29/controls/SO-69/uvd</property>
  <interpolation>
   <entry><ind>0</ind><dep>-30</dep></entry>
   <entry><ind>1</ind><dep>  0</dep></entry>
   <entry><ind>2</ind><dep> 30</dep></entry>
  </interpolation>
  <center>
   <x-m> 0.005</x-m>
   <y-m>-0.040</y-m>
   <z-m> 0.000</z-m>
  </center>
  <axis>
   <x> 0</x>
   <y> 0</y>
   <z>-1</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>SO_RI.lever</object-name>
  <property>mig29/controls/R-862/AM_FM</property>
  <interpolation>
   <entry><ind>0</ind><dep>-20</dep></entry>
   <entry><ind>1</ind><dep> 20</dep></entry>
  </interpolation>
  <center>
   <x-m>-0.020</x-m>
   <y-m>-0.005</y-m>
   <z-m> 0.003</z-m>
  </center>
  <axis>
   <x> 0</x>
   <y>-1</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>SO-69.lever</object-name>
  <property>mig29/controls/SO-69/volna_1-2</property>
  <interpolation>
   <entry><ind>0</ind><dep>-20</dep></entry>
   <entry><ind>1</ind><dep> 20</dep></entry>
  </interpolation>
  <center>
   <x-m>-0.021</x-m>
   <y-m>-0.043</y-m>
   <z-m> 0.003</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>SO_RI.cz1</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/RI/repeat</property>
    <value>1</value>
   </binding>
   <mod-up>
    <binding>
     <command>property-assign</command>
     <property>mig29/controls/RI/repeat</property>
     <value>0</value>
    </binding>
   </mod-up>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>SO_RI.cz2</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/RI/check</property>
    <value>1</value>
   </binding>
   <mod-up>
    <binding>
     <command>property-assign</command>
     <property>mig29/controls/RI/check</property>
     <value>0</value>
    </binding>
   </mod-up>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>SO_RI.cz9</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/SO-69/znak</property>
    <value>1</value>
   </binding>
   <mod-up>
    <binding>
    <command>property-assign</command>
     <property>mig29/controls/SO-69/znak</property>
     <value>0</value>
    </binding>
   </mod-up>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>SO_RI.cz7</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/SO-69/uvd</property>
    <step>-1</step>
    <min>0</min>
    <max>2</max>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>SO_RI.cz8</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/SO-69/uvd</property>
    <step>1</step>
    <min>0</min>
    <max>2</max>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>SO_RI.cz5</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/R-862/AM_FM</property>
    <value>1</value>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>SO_RI.cz6</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/R-862/AM_FM</property>
    <value>0</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>SO_RI.cz3</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/SO-69/volna_1-2</property>
    <value>0</value>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>SO_RI.cz4</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/SO-69/volna_1-2</property>
    <value>1</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>SO_RI.case</object-name>
  <object-name>SO_RI.basebutton1</object-name>
  <object-name>SO_RI.basebutton2</object-name>
  <object-name>SO_RI.basebutton3</object-name>
  <object-name>RI.button1</object-name>
  <object-name>RI.button2</object-name>
  <object-name>SO-69.button</object-name>
  <object-name>SO_RI.baselever</object-name>
  <object-name>SO_RI.lever</object-name>
  <object-name>SO-69.baselever</object-name>
  <object-name>SO-69.lever</object-name>
  <object-name>SO-69.knob</object-name>
  <property>mig29/systems/cockpitlighting/texturep</property>
  <factor>0.5</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
