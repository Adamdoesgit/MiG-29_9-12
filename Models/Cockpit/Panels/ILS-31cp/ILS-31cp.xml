<?xml version="1.0"?>

<PropertyList>

 <path>ILS-31cp.ac</path>

 <animation>
  <type>material</type>
  <object-name>ILS-31cp.case</object-name>
  <object-name>ILS-31cp.basebutton</object-name>
  <object-name>ILS-31cp.button</object-name>
  <object-name>ILS-31cp.knob1</object-name>
  <object-name>ILS-31cp.knob2</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>ILS-31cp.PTA</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-R-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ILS-31cp.knob1</object-name>
  <property>mig29/controls/ILS-31/bright</property>
  <interpolation>
   <entry><ind>0</ind><dep> 0</dep></entry>
   <entry><ind>1</ind><dep>90</dep></entry>
  </interpolation>
  <center>
   <x-m> 0.000</x-m>
   <y-m>-0.045</y-m>
   <z-m> 0.000</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y> 0</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ILS-31cp.knob2</object-name>
  <property>mig29/controls/ILS-31/view-mode</property>
  <interpolation>
   <entry><ind>0</ind><dep>-180</dep></entry>
   <entry><ind>2</ind><dep>0</dep></entry>
  </interpolation>
  <center>
   <x-m>0.000</x-m>
   <y-m>0.044</y-m>
   <z-m>0.000</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y> 0</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>ILS-31cp.button</object-name>
  <property>mig29/controls/ILS-31/test</property>
  <factor>0.003</factor>
  <axis>
   <x>-1</x>
   <y> 0</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>ILS-31cp.cz1</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>true</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/ILS-31/bright</property>
    <step>-0.03</step>
    <min>0.4</min>
    <max>1.0</max>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>ILS-31cp.cz2</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>true</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/ILS-31/bright</property>
    <step>0.03</step>
    <min>0.4</min>
    <max>1.0</max>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>ILS-31cp.cz3</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/ILS-31/test</property>
    <value>1</value>
   </binding>
   <mod-up>
    <binding>
     <command>property-assign</command>
     <property>mig29/controls/ILS-31/test</property>
     <value>0</value>
    </binding>
   </mod-up>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>ILS-31cp.cz4</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/ILS-31/view-mode</property>
    <step>1.0</step>
    <min>0</min>
    <max>2</max>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>ILS-31cp.cz5</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/ILS-31/view-mode</property>
    <step>-1.0</step>
    <min>0</min>
    <max>2</max>
   </binding>
  </action>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>ILS-31cp.case</object-name>
  <object-name>ILS-31cp.basebutton</object-name>
  <object-name>ILS-31cp.button</object-name>
  <object-name>ILS-31cp.knob1</object-name>
  <object-name>ILS-31cp.knob2</object-name>
  <property>mig29/systems/cockpitlighting/texturep</property>
  <factor>0.5</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>