<?xml version="1.0"?>

<PropertyList>

 <path>frp.ac</path>

 <animation>
  <type>material</type>
  <object-name>frp.case</object-name>
  <object-name>frp.sbutton</object-name>
  <object-name>frp.basebutton</object-name>
  <object-name>frp.button</object-name>
  <object-name>frp.baselever</object-name>
  <object-name>frp.lever</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>frp.PTA</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-R-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>frp.lever</object-name>
  <property>mig29/controls/lighting/fary</property>
  <interpolation>
   <entry><ind>0</ind><dep>-30</dep></entry>
   <entry><ind>2</ind><dep>30</dep></entry>
  </interpolation>
  <center>
   <x-m> 0.000</x-m>
   <y-m> 0.0155</y-m>
   <z-m>-0.0295</z-m>
  </center>
  <axis>
   <x> 0</x>
   <y>-1</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>frp.button</object-name>
  <property>mig29/controls/weapons/rp</property>
  <factor>0.002</factor>
  <axis>
   <x>-1</x>
   <y> 0</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>frp.cz2</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/lighting/fary</property>
    <value>2</value>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>frp.cz3</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/lighting/fary</property>
    <value>1</value>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>frp.cz4</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/lighting/fary</property>
    <value>0</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>frp.cz1</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/weapons/rp</property>
    <value>1</value>
   </binding>
   <mod-up>
    <binding>
     <command>property-assign</command>
     <property>mig29/controls/weapons/rp</property>
     <value>0</value>
    </binding>
   </mod-up>
  </action>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>frp.case</object-name>
  <object-name>frp.sbutton</object-name>
  <object-name>frp.basebutton</object-name>
  <object-name>frp.button</object-name>
  <object-name>frp.baselever</object-name>
  <object-name>frp.lever</object-name>
  <property>mig29/systems/cockpitlighting/texturep</property>
  <factor>0.5</factor>
  <step>1</step>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>