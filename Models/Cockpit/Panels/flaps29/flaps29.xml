<?xml version="1.0"?>

<PropertyList>

 <path>flaps29.ac</path>

 <animation>
  <type>material</type>
  <object-name>flaps29.case</object-name>
  <object-name>flaps29.button1</object-name>
  <object-name>flaps29.button2</object-name>
  <object-name>flaps29.button3</object-name>
  <object-name>flaps29.button4</object-name>
  <object-name>flaps29.button5</object-name>
  <object-name>flaps29.lamp</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>flaps29.PTA</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-R-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>flaps29.lampon</object-name>
  <emission>
   <red>  1.0</red>
   <green>1.0</green>
   <blue> 1.0</blue>
   <factor-prop>mig29/systems/cockpitlighting/annunciators-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>select</type>
  <object-name>flaps29.lampon</object-name>
  <condition>
   <equals>
    <property>mig29/systems/flaps/LEF</property>
    <value>0</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>flaps29.cz1</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>/fdm/jsbsim/fcs/slat-cmd-norm</property>
    <value>1</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>flaps29.cz2</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>/fdm/jsbsim/fcs/slat-cmd-norm</property>
    <value>0</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>flaps29.cz3</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>/controls/flight/flaps</property>
    <value>1</value>
   </binding>
   <binding>
    <command>property-assign</command>
    <property>/fdm/jsbsim/fcs/slat-cmd-norm</property>
    <value>1</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>flaps29.cz4</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>/controls/flight/flaps</property>
    <value>1</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>flaps29.cz5</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>/controls/flight/flaps</property>
    <value>0</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>flaps29.case</object-name>
  <object-name>flaps29.button1</object-name>
  <object-name>flaps29.button2</object-name>
  <object-name>flaps29.button3</object-name>
  <object-name>flaps29.button4</object-name>
  <object-name>flaps29.button5</object-name>
  <object-name>flaps29.lamp</object-name>
  <property>mig29/systems/cockpitlighting/texturep</property>
  <factor>0.5</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>