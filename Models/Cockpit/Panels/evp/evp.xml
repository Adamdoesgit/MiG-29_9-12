   <entry><ind>0</ind><dep>-20</dep></entry>
   <entry><ind>1</ind><dep> 20</dep></entry>
  </interpolation>
  <center>
   <x-m>-0.015</x-m>
   <y-m> 0.000</y-m>
   <z-m>-0.015</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y> 0</y>
   <z> 0</z>
  </axis>
 </animation>

