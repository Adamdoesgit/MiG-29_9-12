<?xml version="1.0"?>

<PropertyList>

 <path>evp.ac</path>

 <animation>
  <type>material</type>
  <object-name>evp.case</object-name>
  <object-name>evp.o2</object-name>
  <object-name>evp.baselever1</object-name>
  <object-name>evp.baselever2</object-name>
  <object-name>evp.baselever3</object-name>
  <object-name>evp.baselever4</object-name>
  <object-name>evp.baselever5</object-name>
  <object-name>evp.baselever6</object-name>
  <object-name>evp.baselever7</object-name>
  <object-name>evp.lever1</object-name>
  <object-name>evp.lever2</object-name>
  <object-name>evp.lever3</object-name>
  <object-name>evp.lever4</object-name>
  <object-name>evp.lever5</object-name>
  <object-name>evp.lever6</object-name>
  <object-name>evp.lever7</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>/mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>evp.PTA</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>/mig29/systems/cockpitlighting/panel-R-norm</factor-prop>
  </emission>
 </animation>

 <!-- Аккумулятор, бортовое питание, аэродромное питание -->
 <animation>
  <type>rotate</type>
  <object-name>evp.lever1</object-name>
  <property>/mig29/switches/Akkum-Bort-Aerodrom</property>
  <interpolation>
   <entry><ind>0</ind><dep>-20</dep></entry>
   <entry><ind>1</ind><dep> 20</dep></entry>
  </interpolation>
  <center>
   <x-m>0.010</x-m>
   <y-m>0.000</y-m>
   <z-m>0.015</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y> 0</y>
   <z> 0</z>
  </axis>
 </animation>

 <!-- Генератор постоянного тока -->
 <animation>
  <type>rotate</type>
  <object-name>evp.lever2</object-name>
  <property>/mig29/switches/generator-1</property>
