<?xml version="1.0"?>

<PropertyList>

<path>RUFCcp.ac</path>

 <animation>
  <type>material</type>
  <object-name>RUFCcp.case</object-name>
  <object-name>RUFCcp.baselever</object-name>
  <object-name>RUFCcp.lever</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>RUFCcp.PTA</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-R-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RUFCcp.lever</object-name>
  <property>mig29/controls/Weapons/podv/vnutr-vnesh</property>
  <interpolation>
   <entry><ind>0</ind><dep> 20</dep></entry>
   <entry><ind>1</ind><dep>-20</dep></entry>
  </interpolation>
  <center>
   <x-m> 0.000</x-m>
   <y-m> 0.000</y-m>
   <z-m>-0.0105</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>RUFCcp.cz1</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/Weapons/podv/vnutr-vnesh</property>
    <value>1</value>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>RUFCcp.cz2</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/Weapons/podv/vnutr-vnesh</property>
    <value>0</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>RUFCcp.case</object-name>
  <object-name>RUFCcp.baselever</object-name>
  <object-name>RUFCcp.lever</object-name>
  <property>mig29/systems/cockpitlighting/texturep</property>
  <factor>0.5</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>