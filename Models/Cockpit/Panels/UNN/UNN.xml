<?xml version="1.0"?>

<PropertyList>

<path>UNN.ac</path>

<animation>
 <type>material</type>
 <object-name>UNN.case</object-name>
 <object-name>UNN.b1</object-name>
 <object-name>UNN.b2</object-name>
 <object-name>UNN.b3</object-name>
 <object-name>UNN.baselever</object-name>
 <object-name>UNN.lever</object-name>
 <object-name>UNN.knob1</object-name>
 <object-name>UNN.knob2</object-name>
 <emission>
  <red>0.8</red>
  <green>0.8</green>
  <blue>0.8</blue>
  <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
 </emission>
</animation>

<animation>
 <type>rotate</type>
 <object-name>UNN.lever</object-name>
 <property>mig29/controls/UNN/a</property>
 <interpolation>
  <entry><ind>0</ind><dep>20</dep></entry>
  <entry><ind>1</ind><dep>-20</dep></entry>
 </interpolation>
 <center>
  <x-m> 0.000</x-m>
  <y-m>-0.040</y-m>
  <z-m> 0.028</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>UNN.knob1</object-name>
 <property>mig29/controls/UNN/b</property>
 <interpolation>
  <entry><ind>2</ind><dep>-98</dep></entry>
  <entry><ind>4</ind><dep>-54</dep></entry>
  <entry><ind>6</ind><dep> 54</dep></entry>
  <entry><ind>8</ind><dep> 98</dep></entry>
 </interpolation>
 <center>
  <x-m> 0.000</x-m>
  <y-m>-0.030</y-m>
  <z-m>-0.014</z-m>
 </center>
 <axis>
  <x>-1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>UNN.knob2</object-name>
 <property>mig29/controls/UNN/c</property>
 <interpolation>
  <entry><ind> 0</ind><dep>-150</dep></entry>
  <entry><ind>10</ind><dep> 150</dep></entry>
 </interpolation>
 <center>
  <x-m> 0.000</x-m>
  <y-m> 0.030</y-m>
  <z-m>-0.014</z-m>
 </center>
 <axis>
  <x>-1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>pick</type>
 <object-name>UNN.cz1</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>false</repeatable>
  <binding>
   <command>property-assign</command>
   <property>mig29/controls/UNN/a</property>
   <value>1</value>
  </binding>
 </action>
</animation>
<animation>
 <type>pick</type>
 <object-name>UNN.cz2</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>false</repeatable>
  <binding>
   <command>property-assign</command>
   <property>mig29/controls/UNN/a</property>
   <value>0</value>
  </binding>
 </action>
</animation>
