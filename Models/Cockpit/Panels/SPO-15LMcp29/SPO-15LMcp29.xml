<?xml version="1.0"?>

<PropertyList>

 <path>SPO-15LMcp29.ac</path>

 <animation>
  <type>material</type>
  <object-name>SPO-15LMcp29.case</object-name>
  <object-name>SPO-15LMcp29.baselever1</object-name>
  <object-name>SPO-15LMcp29.baselever2</object-name>
  <object-name>SPO-15LMcp29.lever1</object-name>
  <object-name>SPO-15LMcp29.lever2</object-name>
  <object-name>SPO-15LMcp29.knob</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>SPO-15LMcp29.PTA</object-name>
  <emission>
   <red>  0.8</red>
   <green>0.8</green>
   <blue> 0.8</blue>
   <factor-prop>mig29/systems/cockpitlighting/panel-R-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>SPO-15LMcp29.lever1</object-name>
  <property>mig29/controls/SPO-15LM/scan</property>
  <interpolation>
   <entry><ind>0</ind><dep>-20</dep></entry>
   <entry><ind>1</ind><dep> 20</dep></entry>
  </interpolation>
  <center>
   <x-m>0.005</x-m>
   <y-m>0.015</y-m>
   <z-m>0.000</z-m>
  </center>
  <axis>
   <x> 0</x>
   <y>-1</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>SPO-15LMcp29.lever2</object-name>
  <property>mig29/controls/SPO-15LM/SPO</property>
  <interpolation>
   <entry><ind>0</ind><dep>-20</dep></entry>
   <entry><ind>1</ind><dep> 20</dep></entry>
  </interpolation>
  <center>
   <x-m>0.038</x-m>
   <y-m>0.015</y-m>
   <z-m>0.000</z-m>
  </center>
  <axis>
   <x> 0</x>
   <y>-1</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>SPO-15LMcp29.knob</object-name>
  <property>mig29/controls/SPO-15LM/volume</property>
  <factor>180</factor>
  <center>
   <x-m> 0.015</x-m>
   <y-m>-0.010</y-m>
   <z-m> 0.000</z-m>
  </center>
  <axis>
   <x> 0</x>
   <y> 0</y>
   <z>-1</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>SPO-15LMcp29.cz1</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/SPO-15LM/scan</property>
    <value>1</value>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>SPO-15LMcp29.cz2</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/SPO-15LM/scan</property>
    <value>0</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>SPO-15LMcp29.cz3</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/SPO-15LM/SPO</property>
    <value>1</value>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>SPO-15LMcp29.cz4</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/SPO-15LM/SPO</property>
    <value>0</value>
   </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>SPO-15LMcp29.cz5</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/SPO-15LM/volume</property>
    <step>-0.05</step>
    <min>0</min>
    <max>1</max>
   </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>SPO-15LMcp29.cz6</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/SPO-15LM/volume</property>
    <step>0.05</step>
    <min>0</min>
    <max>1</max>
   </binding>
  </action>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>SPO-15LMcp29.case</object-name>
  <object-name>SPO-15LMcp29.baselever1</object-name>
  <object-name>SPO-15LMcp29.baselever2</object-name>
  <object-name>SPO-15LMcp29.lever1</object-name>
  <object-name>SPO-15LMcp29.lever2</object-name>
  <object-name>SPO-15LMcp29.knob</object-name>
  <property>mig29/systems/cockpitlighting/texturep</property>
  <factor>0.5</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>